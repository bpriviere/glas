dimensions
  dim = 3
  npts = 4
end

parameters
  A(npts, dim+1)
  Q(dim+1, dim+1) diagonal psd
end

variables
  beta (dim + 1)
end

minimize
  quad(beta, Q)
subject to
  A*beta <= -1
end
